<div id="home">
    <!-- navbar -->
    <nav class="navbar navbar-expand-lg navbar bg-info">
        <a class="navbar-brand" id="navbar-profile" href="#">TAKE 5</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
            aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse">
            <div class="navbar-nav ml-auto">
                <a class="nav-item nav-link active" id="link-home" href="#">Home</a>
                <p class="nav-item" id="user-nav">{{username}} • <span id="userNum">{{id}}</span></p>

            </div>
        </div>
    </nav>
    {{!-- jumbotron --}}

    <div class="container" style="padding-top: 2rem">
        <div class="row">
            <div class="col-4">
                <div class="card text-white bg-danger">
                    <h2 class="card-header" id="list-form">New Top 5 List</h2>
                    <div class="card-body">
                        <form action="" id="form-list">

                            <div class="card">
                                <div class="card-header bg-warning">Category
                                    <input type="text" id="category"></div>
                            </div>
                            <div class="card-body">
                                <h5 class="card-title">Favorite 1</h5>
                                <input type="text" id="thing1">
                                <hr>
                                <h5 class="card-title">Favorite 2</h5>
                                <input type="text" id="thing2">
                                <hr>
                                <h5 class="card-title">Favorite 3</h5>
                                <input type="text" id="thing3">
                                <hr>
                                <h5 class="card-title">Favorite 4</h5>
                                <input type="text" id="thing4">
                                <hr>
                                <h5 class="card-title">Favorite 5</h5>
                                <input type="text" id="thing5">
                                <hr>
                                <button id="submitNew5" type="submit">Submit</button>
                            </div>


                    </div>



                </div>

                </form>
            </div>



            <div class="col-4">
                <div class="card text-white bg-primary">
                    <h2 class="card-header">Favorites</h2>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-12" id="favsCol">
                                <div class="card text-dark">
                                    <h4 class="card-header text-dark">Cars</h4>
                                    <div class="card-body">{{favorites.[1]}}</div>
                                    <div class="card-body">Honda•Prius•Camero•Ferrari•Van</div>
                                </div>
                                <hr>
                                <div class="card text-dark">
                                    <h4 class="card-header text-dark">Colors</h4>
                                    <div class="card-body">Green•Red•Chartruese•Yellow•Pantone 292</div>
                                </div>
                                <hr>
                                <div class="card text-dark">
                                    <h4 class="card-header text-dark">Spices</h4>
                                    <div class="card-body">Pepper•Paprika•Cayanne•Black•Curry</div>
                                </div>
                                <hr>
                                <div class="card text-dark">
                                    <h4 class="card-header text-dark">TV Shows</h4>
                                    <div class="card-body">The Simpsons•Alf•ER•Hunter•Aeon Flux</div>
                                </div>

                                <ul id="topList">
                                    {{#each favorites}}
                                    {{> lists/list-block}}
                                    {{/each}}
                                </ul>


                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="col-4">
                <div class="card text-white bg-success">
                    <h2 class="card-header">
                        Following </h2>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-12" id="followCol">
                                <div class="card text-dark">
                                    <h4 class="card-header bg-warning text-dark">Cody</h4>
                                </div>
                                <hr>
                                <div class="card text-dark">
                                    <h4 class="card-header bg-warning text-dark">Stephanie</h4>
                                </div>
                                <hr>
                                <div class="card text-dark">
                                    <h4 class="card-header bg-warning text-dark">David</h4>
                                </div>
                                <hr>
                                <div class="card text-dark">
                                    <h4 class="card-header bg-warning text-dark">Jesus</h4>
                                </div>
                                <hr>
                            </div>
                        </div>
                    </div>
                </div>

            </div>





        </div>



    </div>


</div>
</div>

