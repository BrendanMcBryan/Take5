<div id="home">
    <!-- navbar -->
    <nav class="navbar navbar-expand-lg navbar bg-info">
        <a class="navbar-brand" id="navbar-profile" href="#">TAKE 5</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
            aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse">
            <div class="navbar-nav ml-auto">
                <a class="nav-item nav-link active" id="link-home" href="#">Home</a>
                <p class="nav-item" id="user-nav">{{username}} â€¢ <span id="userNum">{{id}}</span></p>

            </div>
        </div>
    </nav>
    {{!-- jumbotron --}}
    <div class="jumbotron bg-info" id="profile-jumbo">
        <h1 class="display-8" id="list-form">New Top 5 List</h1>
        <hr class="my-4">
        <form action="" id="form-list">
            <ul>
                <li>
                    <p>Category</p>
                    <input type="text" id="category">
                </li>
                <li>
                    <p>Number 1</p>
                    <input type="text" id="thing1">
                </li>
                <p>Number 2</p>
                <input type="text" id="thing2">
                </li>
                <p>Number 3</p>
                <input type="text" id="thing3">
                </li>
                <p>Number 4</p>
                <input type="text" id="thing4">
                </li>
                <p>Number 5</p>
                <input type="text" id="thing5">
                </li>
            </ul>
            <button id="submitNew5" type="submit">Submit</button>
        </form>

    </div>

    <div class="container">
        <div class="row">
            <div class="col-6">
                <div class="card text-white bg-info mb-3" style="max-width: 30rem;">
                    <div class="card-header">Favorites</div>
                    <div class="card-body">
                        <p>Users favorites</p>
                        <ul id="topList">
                            {{#each topLists}}
                            <h3 class="card-title" id="catgeory" data-id="{{this.category}}">Cars</h3>
                            <li>Porsche</li>
                            <li>Maserati</li>
                            <li>Aston Martin</li>
                            <li>Rolls Royce</li>
                            <li>Ferrari</li>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-6">
                <div class="card text-center bg-info">
                    <div class="card-header">
                        Following
                    </div>
                    <div class="card-body">
                        <span>Users following</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>

